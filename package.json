    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "semantic-ui-css": "^2.2.4",
    "style-loader": "^0.13.1",
    "vue-loader": "^11.3.3",
    "vue-template-compiler": "^2.2.6",
    "webpack": "^2.1.0-beta.27",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-md5-hash": "0.0.5"
