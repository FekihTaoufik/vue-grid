{
  "name": "vue-biggrid",
  "version": "1.0.0",
  "description": "A flexible grid component for Vue.js",
  "author": "David Z. Williams <dave@flex.io>",
  "main": "dist/vue-biggrid.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=debug webpack-dev-server --config webpack.config.dev.js --open --inline --hot",
    "build": "cross-env NODE_ENV=debug webpack --config webpack.config.build.js --progress --hide-modules",
    "build-debug": "cross-env NODE_ENV=debug webpack --config webpack.config.build.js --progress --hide-modules",
    "build-release": "cross-env NODE_ENV=production webpack --config webpack.config.build.js --progress --hide-modules",
    "build-production": "cross-env NODE_ENV=production webpack --config webpack.config.build.js --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dzwillia/vue-biggrid.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "components",
    "grid",
    "ui"
  ],
  "dependencies": {
    "lodash": "^4.17.4",
    "tachyons": "^4.6.1",
    "vue": "^2.2.6"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-es2015-destructuring": "^6.19.0",
    "babel-plugin-transform-es2015-parameters": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
